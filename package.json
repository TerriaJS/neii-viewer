    "generate-terriajs-schema": "^1.2.1",
    "glob": "^5.0.5",
    "glob-all": "^3.0.1",
    "gulp": "^3.8.11",
    "gulp-concat": "^2.5.2",
    "gulp-jasmine": "^2.0.1",
    "gulp-jshint": "^1.10.0",
    "gulp-less": "^3.0.3",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.2.0",
