    "gulp-watch": "^4.2.4",
    "less": "^2.5.0",
    "less-plugin-npm-import": "^2.0.0",
    "nationalmap-catalog": "git://github.com/TerriaJS/NationalMap-Catalog",
    "spawn-sync": "^1.0.11",
